<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:complexType name="requestStub">
    <xs:sequence>
      <xs:element name="message" type="xs:string"/>
      <xs:element name="requestId" type="xs:int"/>
      <xs:element name="status" type="statusCode"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="responseStub">
    <xs:sequence>
      <xs:element name="response" type="xs:string"/>
      <xs:element name="responseId" type="xs:int"/>
      <xs:element name="status" type="statusCode"/>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="statusCode">
    <xs:restriction base="xs:string">
      <xs:enumeration value="OK"/>
      <xs:enumeration value="BadRequest"/>
      <xs:enumeration value="Unauthorized"/>
    </xs:restriction>
  </xs:simpleType>
</xs:schema>

